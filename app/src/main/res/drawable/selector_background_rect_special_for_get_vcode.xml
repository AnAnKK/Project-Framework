<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_FFFFFF" />
            <stroke android:width="@dimen/qb_px_2" android:color="@color/color_E5E5E5" />
            <corners android:bottomLeftRadius="@dimen/qb_px_4" android:bottomRightRadius="@dimen/qb_px_4" android:topLeftRadius="@dimen/qb_px_4" android:topRightRadius="@dimen/qb_px_4" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <solid android:color="@color/color_primary" />
            <stroke android:width="@dimen/qb_px_2" android:color="@color/color_primary" />
            <corners android:bottomLeftRadius="@dimen/qb_px_4" android:bottomRightRadius="@dimen/qb_px_4" android:topLeftRadius="@dimen/qb_px_4" android:topRightRadius="@dimen/qb_px_4" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#f4f4f4" />
            <stroke android:width="@dimen/qb_px_2" android:color="#cccccc" />
            <corners android:bottomLeftRadius="@dimen/qb_px_4" android:bottomRightRadius="@dimen/qb_px_4" android:topLeftRadius="@dimen/qb_px_4" android:topRightRadius="@dimen/qb_px_4" />
        </shape>
    </item>
</selector>
